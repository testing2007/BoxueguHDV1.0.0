#import <Foundation/Foundation.h>

@interface BXGStudyStatusModel : NSObject

@property (nonatomic, strong) NSString *userId;
@property (nonatomic, strong) NSString *courseId;
@property (nonatomic, strong) NSString *phaseId;
@property (nonatomic, strong) NSString *moduleId;
@property (nonatomic, strong) NSString *sectionId;
@property (nonatomic, strong) NSString *pointId;
@property (nonatomic, strong) NSString *videoId;
@property (nonatomic, strong) NSString *videoName;
@property (nonatomic, assign) NSInteger isSync;
@property (nonatomic, assign) NSInteger status;
@property (nonatomic, assign) double seekTime;
@property (nonatomic, assign) double durantionTime;
@property (nonatomic, strong) NSString *addtime; // @"yyyy-MM-dd HH:mm:ss.SSS"

- (instancetype)initWithUserId:(NSString *)userId
                      courseId:(NSString *)courseId
                       phaseId:(NSString *)phaseId
                      moduleId:(NSString *)moduleId
                     sectionId:(NSString *)sectionId
                       pointId:(NSString *)pointId
                       videoId:(NSString *)videoId
                     videoName:(NSString *)videoName
                        isSync:(NSInteger)isSync
                        status:(NSInteger)status
                      seekTime:(double)seekTime
                  durationTime:(double)durationTime
                       addTime:(NSString *)addtime;
@end
